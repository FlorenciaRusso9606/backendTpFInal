{
  "compilerOptions": {
    "outDir": "dist",
    "moduleResolution": "node",
    "module": "CommonJS",
    "target": "ES2022",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "typeRoots": ["./src/types", "./node_modules/@types"],
    "types": ["node", "express"]
  },
  "include": ["src/**/*.ts", "scripts/**/*.ts", "src/types", "temp-check.ts"]

}
